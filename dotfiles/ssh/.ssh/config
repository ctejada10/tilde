UseRoaming no

Host *
  AddKeysToAgent yes
  ForwardAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  ServerAliveInterval 60
  IdentityFile ~/.ssh/arnor

Host */*
  ProxyCommand ssh $(dirname %h) -W $(basename %h):%p

Host rivendell
  HostName rivendell.middlearth.io
  ForwardAgent yes
  User ctejada
  Port 69
  ExitOnForwardFailure yes

Host esgaroth
  HostName esgaroth.middlearth.io
  ForwardAgent yes
  User ctejada
  ExitOnForwardFailure yes